{
    "_comment": "FIXME XXX: remove version db_usertestdb in not testing environment",
    "name": "webbackend",
    "description": "Webmail HTTP server and API",
    "copyright": "Copyright Â© 2014, Juanjo Alvarez Martinez",
    "authors": ["juanjux"],
    "importPaths": [".", ".."],
    "sourcePaths": [".", ".."],
    "excludedSourceFiles": ["../retriever/main.d", "../retriever/test.d", "livetests.d", "test.d"],
    "dependencies": {
        "vibe-d": "~master"
    },
    "versions": ["VibeDefaultMain", "db_usetestdb"],
    "buildOptions": ["debugMode"],
    "configurations": [
        {
            "name": "default",
            "targetType": "executable",
            "platforms": ["posix"],
        },
        {
            "name": "withtestdb",
            "targetType": "executable",
            "platforms": ["posix"],
            "versions": ["db_usetestdb"],
        },
        {
            "name": "apitest",
            "versions": ["apitest", "db_usetestdb"],
            "dflags": ["-unittest", "-debug"],
            "targetName": "test",
            "targetType": "executable",
            "platforms": ["posix"],
        },
        {
            "name": "curltest",
            "versions": ["apitest", "db_usetestdb"],
            "dflags": ["-debug"],
            "targetName": "test",
            "targetType": "executable",
            "platforms": ["posix"],
        },
    ]
}
